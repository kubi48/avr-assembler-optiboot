     	ldi	r20, WATCHDOG_16MS
    3fec:	48 e0       	ldi	r20, 0x08	; 8
	rcall	watchdogConfig	;  watchdogConfig(WATCHDOG_16MS) 
    3fee:	f8 df       	rcall	.-16     	; 0x3fe0 <watchdogConfig>

00003ff0 <lop77>:
lop77:
    	rjmp	lop77		; endless loop, watch Dog will reset!
    3ff0:	ff cf       	rjmp	.-2      	; 0x3ff0 <lop77>
; 
; FORCE_WATCHDOG=
; LED_START_FLASHES=0
; LED_DATA_FLASH=4
; LED=B5
; UART=0
; SOURCE_TYPE=S
; SUPPORT_EEPROM=1
; MCU_TARGET = atmega168
; AVR_FREQ= 16000000
; BAUD_RATE=36
